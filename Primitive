#include <stdio.h>
#include <math.h>
#define MAX 500
#define NUM 50

int main()
{
    char line[MAX] = {0};
    gets(line);
    int first = 0, second = 0, trird = 0, fourth = 0;
    int item_trig = 0;
    char items[NUM] = {0}, boxes[NUM] = {0};
    int z = 0, x = 0;
    for(int i = 0; line[i] != '\0'; i++)
    {
        if((line[i] == 'h' || line[i] == 'H') && line[i + 1] == 'o' && line[i + 2] == 'w')
            first = 1;
        if(line[i] == 'm' && line[i + 1] == 'a' && line[i + 2] == 'n')
            second = 1;
        if(line[i] == 'w' && line[i + 1] == 'a' && line[i + 2] == 'y')
            trird = 1;
        if(item_trig == 0 && line[i] >= '0' && line[i] <= '9')
        {
            items[z] = line[i];
            z++;
        }
        if(z != 0 && (line[i] < '0' || line[i] > '9'))
        {
            item_trig = i;
            break;
        }
    }
    for(int i = 0; line[i] != '\0'; i++)
    {
        if(z != 0 && line[i] == 'i' && line[i + 1] == 'n')
            fourth = 1;
    }
    for(int i = 0; line[i] != '\0'; i++)
    {
        if(item_trig != 0 && i > item_trig && line[i] >= '0' && line[i] <= '9')
        {
            boxes[x] = line[i];
            x++;
            if(line[i] < '0' || line[i] > '9')
                break;
        }
    }
    if(!(first == 1 && second == 1 && trird == 1 && fourth == 1))
    {
        printf("Can't define query!\n");
        return 0;
    }
    int subs = 0, niches = 0;
    for(int i = 0; items[i] != '\0'; i++)
    {
        z--;
        subs += (items[i] - 48) * pow(10, z);
    }
    for(int i = 0; boxes[i] != '\0'; i++)
    {
        x--;
        niches += (boxes[i] - 48) * pow(10, x);
    }
    int res = pow(niches, subs);
    printf(" = %d\n", res);
    return 0;
}
